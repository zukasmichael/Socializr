{
    "minimum-stability": "dev",
    "name": "ISDTCLIENT",
    "description": "Windesheim portfolio ISDTCLIENT",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan de Rooij"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "silex/silex": "~1.1",
        "symfony/console": "~2.0",
        "doctrine/annotations": "~1.0",
        "doctrine/collections": "~1.0",
        "doctrine/common": "2.4.*",
        "doctrine/cache": "~1.0",
        "doctrine/inflector": "~1.0",
        "doctrine/mongodb": "1.0.*",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "neutron/mongo-odm-silex-provider": "*",
        "symfony/form": "~2.3",
        "symfony/security": "~2.3",
        "gigablah/silex-oauth": "~0.1",
        "monolog/monolog"       : "~1.0"
    },
    "require-dev": {
        "symfony/yaml": "~2.0"
    },
    "suggest": {
        "symfony/yaml": "Enables the YAML metadata mapping driver"
    },
    "autoload": {
        "psr-0": {
            "Doctrine\\ODM\\MongoDB": "lib/",
            "Models": "app/"
        }
    }
}